{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "cache": {
      "enabled": false
    },
    "analytics": false
  },
  "projects": {
    "admin-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/admin-ui",
      "sourceRoot": "projects/admin-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/admin-ui",
            "index": "projects/admin-ui/src/index.html",
            "browser": "projects/admin-ui/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/admin-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "projects/admin-ui/src/styles.scss"
            ],
            "scripts": [
              "node_modules/apexcharts/dist/apexcharts.min.js"
            ],
            "allowedCommonJsDependencies": [
              "validator",
              "moment"
            ]
          },
          "configurations": {
            "production": {
              "assets": [
                "projects/admin-ui/src/favicon.png",
                "projects/admin-ui/src/assets",
                {
                  "glob": "env.js",
                  "input": "projects/admin-ui/src/environments/production",
                  "output": "/"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "assets": [
                "projects/admin-ui/src/favicon.png",
                "projects/admin-ui/src/assets",
                {
                  "glob": "env.js",
                  "input": "projects/admin-ui/src/environments/dev",
                  "output": "/"
                }
              ]
            },
            "stage": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "assets": [
                "projects/admin-ui/src/favicon.png",
                "projects/admin-ui/src/assets",
                {
                  "glob": "env.js",
                  "input": "projects/admin-ui/src/environments/stage",
                  "output": "/"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "admin-ui:build:production"
            },
            "development": {
              "buildTarget": "admin-ui:build:development"
            },
            "stage": {
              "buildTarget": "admin-ui:build:stage"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "admin-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/admin-ui/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin-ui/src/favicon.ico",
              "projects/admin-ui/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/pink-bluegrey.css",
              "projects/admin-ui/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/admin-ui/**/*.ts",
              "projects/admin-ui/**/*.html"
            ]
          }
        }
      }
    },
    "user-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/user-ui",
      "sourceRoot": "projects/user-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/user-ui",
            "index": "projects/user-ui/src/index.html",
            "browser": "projects/user-ui/src/main.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "projects/user-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "projects/user-ui/src/styles.scss",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "allowedCommonJsDependencies": [
              "validator",
              "moment"
            ]
          },
          "configurations": {
            "production": {
              "assets": [
                "projects/user-ui/src/favicon.png",
                "projects/user-ui/src/assets",
                "projects/user-ui/src/robots.txt",
                "projects/user-ui/src/sitemap.xml",
                {
                  "glob": "env.js",
                  "input": "projects/user-ui/src/environments/production",
                  "output": "/"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "assets": [
                "projects/user-ui/src/favicon.png",
                "projects/user-ui/src/assets",
                "projects/user-ui/src/robots.txt",
                "projects/user-ui/src/sitemap.xml",
                {
                  "glob": "env.js",
                  "input": "projects/user-ui/src/environments/dev",
                  "output": "/"
                }
              ],
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "stage": {
              "assets": [
                "projects/user-ui/src/favicon.png",
                "projects/user-ui/src/assets",
                "projects/user-ui/src/robots.txt",
                "projects/user-ui/src/sitemap.xml",
                {
                  "glob": "env.js",
                  "input": "projects/user-ui/src/environments/stage",
                  "output": "/"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "user-ui:build:production"
            },
            "development": {
              "buildTarget": "user-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "user-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing",
              "@angular/localize/init"
            ],
            "tsConfig": "projects/user-ui/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/user-ui/src/favicon.ico",
              "projects/user-ui/src/assets"
            ],
            "styles": [
              "projects/user-ui/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/user-ui/**/*.ts",
              "projects/user-ui/**/*.html"
            ]
          }
        }
      }
    }
  }
}