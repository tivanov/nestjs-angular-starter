{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Dev Script",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": ["${workspaceFolder}/**/*.(m|c|)js", "!**/node_modules/**"]
    }
  ]
}
